import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int r=n%10;
        int m;
        if(r<5)
        {
           m=n-r;
        }
        else
        {
            m=n+(10-r);
        }
        System.out.println(m);
    }
}
